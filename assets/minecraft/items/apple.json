{
  "model": {
    "type": "select",
    "property": "custom_model_data",
    "fallback": {
      "type": "model",
      "model": "item/apple"
    },
    "cases": [
      {
        "when": "1",
        "model": {
          "type": "model",
          "model": "item/apple_baby"
        }
      },
      {
        "when": "2",
        "model": {
          "type": "model",
          "model": "item/pear_baby"
        }
      }
    ]
  }
}